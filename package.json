{
  "name": "template-reactjs",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@commitlint/config-conventional": "^18.1.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@material-ui/core": "^5.0.0-beta.5",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "axios": "^1.6.0",
    "customize-cra": "^1.0.0",
    "env-cmd": "^10.1.0",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "mobx": "6.10.2",
    "mobx-persist": "^0.4.1",
    "mobx-react": "^9.0.1",
    "mobx-react-router": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.2",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "styled-components": "^6.1.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "dev": "env-cmd -f .env.dev yarn react-app-rewired start",
    "hml": "env-cmd -f .env.hml yarn react-app-rewired start",
    "prd": "env-cmd -f .env.prd yarn react-app-rewired start",
    "build:dev": "env-cmd -f .env.dev yarn react-app-rewired build",
    "build:hml": "env-cmd -f .env.hml yarn react-app-rewired build",
    "build:prd": "env-cmd -f .env.prd yarn react-app-rewired build",
    "test": "react-app-rewired test",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "pretty": "prettier --config ./.prettierrc --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "tsc && eslint \"./src/**/*.{js,jsx,ts,tsx}\" --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "*.{ts,tsx,js, jsx}": [
      "eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.200",
    "@types/react-dom": "^18.2.14",
    "@types/styled-components": "^5.1.29",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-root-import": "^6.6.0",
    "commitlint": "^18.2.0",
    "eslint": "^8.53.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lint-staged": "^15.0.2",
    "prettier": "3.0.3",
    "react-app-rewired": "^2.2.1",
    "reactotron-react-js": "^3.3.8"
  }
}
